<?xml version='1.0'?>
<sdf version='1.6'>
<model name='tape_court'>
  <static>true</static>

  <!-- columns -->
  <% columns.each do |column| %>
  <include>
    <uri><%= column[:uri] %></uri>
    <pose><%= column[:pose] %></pose>
  </include>
  <% end %>

  <!-- rows -->
  <% rows.each do |row| %>
  <include>
    <uri><%= row[:uri] %></uri>
    <pose><%= row[:pose] %></pose>
  </include>
  <% end %>

</model>
</sdf>
